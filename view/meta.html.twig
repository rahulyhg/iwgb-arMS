<meta property="og:title" content="{{ title }}">
<meta property="og:url" content="http://{{ url }}">
{% if image != '' %}
	<meta property="og:image" content="http://iwgb.co.uk/img/bucket/{{ image }}.jpg">
	<meta name="twitter:image" content="http://iwgb.co.uk/img/bucket/{{ image }}.jpg">
{% endif %}
<meta name="twitter:site" content="@IWGBUnion">
<meta name="twitter:title" content="IWGB">
<meta name="twitter:description" content="The IWGB is the UK's leading trade union for gig economy and outsourced workers.">
