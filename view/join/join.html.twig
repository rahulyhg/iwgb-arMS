{% extends 'page.html.twig' %}

{#
	joinCopy: dynamic page elements for this page conformant to pages.schema.json#/definitions/page
	branches: list of branch data conformant to branches.schema.json
#}

{% block head %}
	<title>Join &middot; IWGB</title>
{% endblock %}

{% block page %}
	{% include 'branches.html.twig' with {
		'title': 		'Join a branch as a member',
		'joinlinks': 	true,
		'branches':		branches
	} only %}

	{% include 'page-element.html.twig' with joinCopy.elements[0] only %}

{% endblock %}
